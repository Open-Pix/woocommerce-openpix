  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn prettier",
      "eslint --fix"
    ],
    "*.yml": [
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.php": [
      "prettier --write"
    ]
  },
  "main": "index.js",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production COMMIT_SHA=$(git rev-parse HEAD) webpack --config webpack.prod.config.js --progress",
    "build:staging": "cross-env NODE_ENV=production COMMIT_SHA=$(git rev-parse HEAD) webpack --config webpack.staging.config.js --progress",
    "config:local": "cp .env.local .env",
    "es": "node -r esbuild-register",
    "lint": "eslint --ext .ts --ext .tsx 'checkout/src/**'",
    "order": "vite order",
    "prepare": "husky install",
    "prettier": "prettier --write",
    "release:major": "yarn es ./scripts/changelog --major",
    "release:minor": "yarn es ./scripts/changelog --minor",
    "release:patch": "yarn es ./scripts/changelog --patch",
    "serve": "node server.js",
    "start": "webpack serve --hot --config webpack.config.js --progress",
    "test": "yarn jest",
    "type-check": "tsc --noEmit"
  }
